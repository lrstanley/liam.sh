{
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "dependencies": {
    "@codemirror/lang-markdown": "^6.0.0",
    "@codemirror/theme-one-dark": "^6.0.0",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-vue-urql": "^2.3.3",
    "@iconify-json/mdi": "^1.1.31",
    "@tiptap/starter-kit": "2.0.0-beta.192",
    "@tiptap/vue-3": "2.0.0-beta.97",
    "@types/node": "^18.7.15",
    "@urql/exchange-retry": "^0.3.3",
    "@urql/vue": "^0.6.3",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/components": "^9.2.0",
    "@vueuse/core": "^9.2.0",
    "@vueuse/motion": "^2.0.0-beta.12",
    "@vueuse/router": "^9.2.0",
    "autoprefixer": "^10.4.7",
    "codemirror": "^6.0.0",
    "core-js": "^3.25.0",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "naive-ui": "^2.33.2",
    "pinia": "^2.0.21",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "unplugin-auto-import": "^0.10.3",
    "unplugin-icons": "^0.14.9",
    "unplugin-vue-components": "^0.21.2",
    "vite": "^3.1.0",
    "vite-plugin-pages": "^0.25.0",
    "vue": "^3.2.38",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-tailwindcss": "^3.6.1",
    "eslint-plugin-vue": "^9.4.0",
    "prettier": "^2.7.1"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended",
      "plugin:tailwindcss/recommended",
      "prettier",
      "./.eslintrc-auto-import.json"
    ],
    "globals": {
      "defineEmits": "readonly",
      "defineExpose": "readonly",
      "defineProps": "readonly",
      "withDefaults": "readonly"
    },
    "ignorePatterns": [
      "dist",
      "node_modules",
      ".git"
    ],
    "root": true,
    "rules": {
      "newline-per-chained-call": "off",
      "tailwindcss/classnames-order": "off",
      "tailwindcss/enforces-negative-arbitrary-values": "error",
      "tailwindcss/enforces-shorthand": "error",
      "tailwindcss/migration-from-tailwind-2": "error",
      "tailwindcss/no-arbitrary-value": "off",
      "tailwindcss/no-contradicting-classname": "error",
      "tailwindcss/no-custom-classname": "warn",
      "vue/html-indent": "off",
      "vue/max-attributes-per-line": "off",
      "vue/multi-word-component-names": "off",
      "vue/no-v-html": "off",
      "vue/singleline-html-element-content-newline": "off"
    }
  },
  "name": "liam.sh",
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "quoteProps": "as-needed",
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "useTabs": false,
    "vueIndentScriptAndStyle": false
  },
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm"
  },
  "version": "1.0.0"
}
