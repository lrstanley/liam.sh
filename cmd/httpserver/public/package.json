{
  "name": "liam.sh",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preserver": "graphql-codegen",
    "server": "npx vite",
    "prepreview": "npx vite build",
    "preview": "npx vite preview",
    "prebuild": "graphql-codegen",
    "build": "npx vite build",
    "lint": "npx eslint . --ext .js,.vue"
  },
  "dependencies": {
    "@codemirror/lang-markdown": "^6.0.0",
    "@codemirror/theme-one-dark": "^6.0.0",
    "@urql/exchange-retry": "^0.3.3",
    "@urql/vue": "^0.6.3",
    "@vueuse/components": "^8.7.5",
    "@vueuse/core": "^8.7.5",
    "@vueuse/motion": "^2.0.0-beta.12",
    "@vueuse/router": "^8.7.5",
    "codemirror": "^6.0.0",
    "core-js": "^3.23.2",
    "graphql": "^16.5.0",
    "naive-ui": "^2.30.6",
    "pinia": "^2.0.14",
    "vue": "^3.2.37",
    "vue-codemirror": "^6.0.0",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@graphql-codegen/typescript-vue-urql": "^2.2.14",
    "@iconify-json/mdi": "^1.1.22",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "eslint-plugin-vue": "^9.1.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.6",
    "unplugin-auto-import": "^0.9.0",
    "unplugin-icons": "^0.14.5",
    "unplugin-vue-components": "^0.19.9",
    "vite": "^3.0.0",
    "vite-plugin-pages": "^0.25.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended",
      "plugin:tailwindcss/recommended",
      "prettier",
      "./.eslintrc-auto-import.json"
    ],
    "rules": {
      "newline-per-chained-call": "off",
      "vue/html-indent": "off",
      "vue/max-attributes-per-line": "off",
      "vue/multi-word-component-names": "off",
      "vue/singleline-html-element-content-newline": "off",
      "vue/no-v-html": "off",
      "tailwindcss/classnames-order": "off",
      "tailwindcss/enforces-negative-arbitrary-values": "error",
      "tailwindcss/enforces-shorthand": "error",
      "tailwindcss/migration-from-tailwind-2": "error",
      "tailwindcss/no-arbitrary-value": "off",
      "tailwindcss/no-custom-classname": "warn",
      "tailwindcss/no-contradicting-classname": "error"
    },
    "ignorePatterns": [
      "dist",
      "node_modules",
      ".git"
    ],
    "globals": {
      "defineProps": "readonly",
      "defineEmits": "readonly",
      "defineExpose": "readonly",
      "withDefaults": "readonly"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "arrowParens": "always",
    "quoteProps": "as-needed",
    "singleQuote": false,
    "useTabs": false,
    "semi": false,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "vueIndentScriptAndStyle": false
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
