{
  "name": "liam.sh",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preserver": "graphql-codegen",
    "server": "npx vite",
    "prepreview": "npx vite build --base /",
    "preview": "npx vite preview",
    "prebuild": "graphql-codegen",
    "build": "npx vite build --base /dist",
    "lint": "npx eslint . --ext .js,.vue"
  },
  "dependencies": {
    "@codemirror/lang-markdown": "^0.20.0",
    "@codemirror/theme-one-dark": "^0.20.0",
    "@urql/exchange-retry": "^0.3.3",
    "@urql/vue": "^0.6.3",
    "@vueuse/components": "8.5.0",
    "@vueuse/core": "8.5.0",
    "@vueuse/motion": "^2.0.0-beta.12",
    "@vueuse/router": "8.5.0",
    "core-js": "^3.22.8",
    "graphql": "^16.5.0",
    "naive-ui": "^2.30.3",
    "pinia": "^2.0.14",
    "vue": "^3.2.37",
    "vue-codemirror": "^5.0.1",
    "vue-router": "^4.0.16",
    "windicss": "^3.5.4"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.5.0",
    "@graphql-codegen/typescript-operations": "^2.4.1",
    "@graphql-codegen/typescript-vue-urql": "^2.2.13",
    "@iconify-json/mdi": "^1.1.19",
    "@vitejs/plugin-vue": "^2.3.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "unplugin-auto-import": "^0.8.7",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.6",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-windicss": "^1.8.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended",
      "prettier",
      "./.eslintrc-auto-import.json"
    ],
    "rules": {
      "vue/html-indent": "off",
      "vue/max-attributes-per-line": "off",
      "vue/multi-word-component-names": "off",
      "vue/singleline-html-element-content-newline": "off",
      "newline-per-chained-call": "off",
      "vue/no-v-html": "off"
    },
    "ignorePatterns": [
      "dist",
      "node_modules",
      ".git"
    ],
    "globals": {
      "defineProps": "readonly",
      "defineEmits": "readonly",
      "defineExpose": "readonly",
      "withDefaults": "readonly"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "arrowParens": "always",
    "quoteProps": "as-needed",
    "singleQuote": false,
    "useTabs": false,
    "semi": false,
    "bracketSpacing": true,
    "brackeSameLine": false,
    "vueIndentScriptAndStyle": false
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
