{
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    ".git"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "naive-ui/volar"
    ]
  },
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true
  }
}
