{
  "name": "liam.sh",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preserver": "graphql-codegen",
    "server": "npx --max_old_space_size=4096 vite --mode development --base /",
    "preview": "npx --max_old_space_size=4096 vite preview --mode production --base / --port 8081 --strictPort",
    "prebuild": "graphql-codegen",
    "build": "npx --max_old_space_size=4096 vite build --mode production --base /dist --emptyOutDir",
    "lint": "npx eslint . --ext .js,.vue"
  },
  "dependencies": {
    "@graphql-codegen/near-operation-file-preset": "^2.2.12",
    "@urql/exchange-retry": "^0.3.3",
    "@urql/vue": "^0.6.2",
    "@vueuse/core": "^8.2.6",
    "@vueuse/motion": "^2.0.0-beta.12",
    "axios": "^0.27.2",
    "core-js": "^3.8.3",
    "graphql": "^16.5.0",
    "naive-ui": "^2.28.2",
    "pinia": "^2.0.13",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "windicss": "^3.5.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@graphql-codegen/typescript-vue-urql": "^2.2.12",
    "@iconify-json/mdi": "^1.1.9",
    "@urql/devtools": "^2.0.3",
    "@vitejs/plugin-vue": "^2.3.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.0.3",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.3",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-windicss": "^1.8.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended",
      "prettier",
      "./.eslintrc-auto-import.json"
    ],
    "rules": {
      "vue/html-indent": "off",
      "vue/max-attributes-per-line": "off",
      "vue/multi-word-component-names": "off",
      "vue/singleline-html-element-content-newline": "off",
      "newline-per-chained-call": "off"
    },
    "ignorePatterns": [
      "dist",
      "node_modules",
      ".git"
    ],
    "globals": {
      "defineProps": "readonly",
      "defineEmits": "readonly",
      "defineExpose": "readonly",
      "withDefaults": "readonly"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "arrowParens": "always",
    "quoteProps": "as-needed",
    "singleQuote": false,
    "useTabs": false,
    "semi": false,
    "bracketSpacing": true,
    "brackeSameLine": false,
    "vueIndentScriptAndStyle": false
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
