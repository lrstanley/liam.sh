{
  "name": "liam.sh",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "server": "npx --max_old_space_size=4096 vite --mode development --base /",
    "preview": "npx --max_old_space_size=4096 vite preview --mode production --base / --port 8081 --strictPort",
    "build": "npx --max_old_space_size=4096 vite build --mode production --base /dist --emptyOutDir",
    "openapi-generate": "npx openapi --input ../internal/ent/openapi.json --output src/lib/http/api --client axios --name ApiClient",
    "lint": "npx eslint . --ext .js,.vue"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@sicons/ionicons5": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vueuse/core": "^8.2.6",
    "@vueuse/motion": "^2.0.0-beta.12",
    "axios": "^0.27.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.0.3",
    "form-data": "^4.0.0",
    "naive-ui": "^2.28.2",
    "openapi-typescript-codegen": "^0.21.0",
    "pinia": "^2.0.13",
    "postcss": "^8.4.12",
    "prettier": "^2.4.1",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-windicss": "^1.8.4",
    "windicss": "^3.5.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended",
      "./.eslintrc-auto-import.json"
    ],
    "rules": {
      "vue/html-indent": "off",
      "vue/max-attributes-per-line": "off",
      "vue/multi-word-component-names": "off",
      "vue/singleline-html-element-content-newline": "off",
      "newline-per-chained-call": "off"
    },
    "ignorePatterns": [
      "dist",
      "node_modules",
      ".git"
    ]
  },
  "prettier": {
    "printWidth": 140,
    "tabWidth": 4,
    "trailingComma": "es5",
    "arrowParens": "always",
    "quoteProps": "as-needed",
    "singleQuote": false,
    "useTabs": false,
    "semi": false,
    "bracketSpacing": true,
    "brackeSameLine": false,
    "vueIndentScriptAndStyle": false
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
